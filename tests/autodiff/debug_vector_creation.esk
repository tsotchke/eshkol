;; Debug: Test vector creation and dimension reading

(define (test-vector-creation)
  (let ((v (vector 3.0)))
    (display "Created vector with element 3.0")
    (newline)
    (display "Vector element at index 0: ")
    (display (vref v 0))
    (newline)))

(define (test-2d-vector)
  (let ((v (vector 2.0 3.0)))
    (display "Created 2D vector [2.0, 3.0]")
    (newline)
    (display "Element 0: ")
    (display (vref v 0))
    (newline)
    (display "Element 1: ")
    (display (vref v 1))
    (newline)))

(display "=== Vector Creation Debug ===")
(newline)
(test-vector-creation)
(test-2d-vector)
(display "=== Done ===")
(newline)