;; Test gradient and jacobian

(define (main)
  (display "Testing gradient...\n")
  (let ((result (gradient (lambda (v) 0) (vector 1.0 2.0))))
    (display result)
    (newline))
  (display "Gradient done\n")

  (display "Testing jacobian...\n")
  (let ((result (jacobian (lambda (v) (vector 1.0 2.0)) (vector 1.0 2.0))))
    (display result)
    (newline))
  (display "Jacobian done\n")
  0)
