;;; Test refactored Eshkol stdlib functions
(require stdlib)

(display "=== Testing Refactored Stdlib Functions ===")
(newline)

;;; Test find
(display "find even? '(1 3 4 5 6): ")
(display (find even? '(1 3 4 5 6)))
(newline)

(display "find even? '(1 3 5): ")
(display (find even? '(1 3 5)))
(newline)

;;; Test member?
(display "member? 3 '(1 2 3 4 5): ")
(display (member? 3 '(1 2 3 4 5)))
(newline)

(display "member? 9 '(1 2 3 4 5): ")
(display (member? 9 '(1 2 3 4 5)))
(newline)

;;; Test length
(display "length '(1 2 3 4 5): ")
(display (length '(1 2 3 4 5)))
(newline)

(display "length '(): ")
(display (length '()))
(newline)

;;; Test last
(display "last '(1 2 3 4 5): ")
(display (last '(1 2 3 4 5)))
(newline)

;;; Test last-pair
(display "last-pair '(1 2 3): ")
(display (last-pair '(1 2 3)))
(newline)

;;; Test take
(display "take '(1 2 3 4 5) 3: ")
(display (take '(1 2 3 4 5) 3))
(newline)

(display "take '(1 2) 5: ")
(display (take '(1 2) 5))
(newline)

;;; Test drop
(display "drop '(1 2 3 4 5) 2: ")
(display (drop '(1 2 3 4 5) 2))
(newline)

;;; Test append
(display "append '(1 2 3) '(4 5 6): ")
(display (append '(1 2 3) '(4 5 6)))
(newline)

;;; Test reverse
(display "reverse '(1 2 3 4 5): ")
(display (reverse '(1 2 3 4 5)))
(newline)

;;; Test filter
(display "filter even? '(1 2 3 4 5 6): ")
(display (filter even? '(1 2 3 4 5 6)))
(newline)

;;; Test remove
(display "remove 3 '(1 2 3 4 3 5): ")
(display (remove 3 '(1 2 3 4 3 5)))
(newline)

;;; Test count-if
(display "count-if even? '(1 2 3 4 5 6): ")
(display (count-if even? '(1 2 3 4 5 6)))
(newline)

(display "=== All Tests Complete ===")
(newline)
