;;
;; Association List Functions Test
;;

(require stdlib)

(display "Testing association list functions:")
(newline)

;; Create association list: ((a . 1) (b . 2) (c . 3))
(define alist (list (cons 10 100) (cons 20 200) (cons 30 300)))

(display "Association list created:")
(newline)
(display "First pair: ") (display (car (car alist))) (display " -> ") (display (cdr (car alist)))
(newline)
(display "Second pair: ") (display (car (cadr alist))) (display " -> ") (display (cdr (cadr alist)))
(newline)

;; Test assoc function
(display "Testing assoc:")
(newline)

(define found-20 (assoc 20 alist))
(display "assoc 20: ")
(if (null? found-20)
    (display "NOT FOUND")
    (display "FOUND")
)
(newline)

(define not-found (assoc 99 alist))
(display "assoc 99: ")
(if (null? not-found)
    (display "NOT FOUND")
    (display "FOUND")
)
(newline)

(display "Association test completed!")
(newline)
