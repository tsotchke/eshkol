;; Test without displaying rest-args
(require stdlib)

(define (lists-any-null? lists)
  (if (null? lists) #f (null? (car lists))))

(define (all-cars lists)
  (if (null? lists) '() (cons (car (car lists)) (all-cars (cdr lists)))))

(define (all-cdrs lists)
  (if (null? lists) '() (cons (cdr (car lists)) (all-cdrs (cdr lists)))))

(define (vmap proc . lists)
  (display "lists=")
  (display lists)
  (newline)
  (if (lists-any-null? lists)
      '()
      (begin
        (display "recursive")
        (newline)
        (define cars (all-cars lists))
        (display "cars=")
        (display cars)
        (newline)
        (define cdrs (all-cdrs lists))
        (display "cdrs=")
        (display cdrs)
        (newline)
        (define first-result (apply proc cars))
        (display "first-result=")
        (display first-result)
        (newline)
        (define rest-args (cons proc cdrs))
        ;; NOT displaying rest-args
        (cons first-result (apply vmap rest-args)))))

(display "Starting test")
(newline)
(define (double x) (* x 2))
(display (vmap double (list 1 2)))
(newline)
