;; Simple map debugging test

(require stdlib)

(display "=== MAP DEBUG TEST ===")
(newline)

;; Test simple lists first
(define test-list1 (list 1 2))
(define test-list2 (list 10 20))
(display "List1: ")
(display (car test-list1)) (display " ") (display (cadr test-list1))
(newline)
(display "List2: ")
(display (car test-list2)) (display " ") (display (cadr test-list2))
(newline)

;; Test map with 2 lists (problematic case)
(define map-result (map + test-list1 test-list2))
(display "Map result: ")
(display (car map-result))
(if (null? (cdr map-result))
    (display " (only 1 element)")
    (begin
        (display " ")
        (display (cadr map-result))
    ))
(newline)

(display "=== END DEBUG ===")
(newline)