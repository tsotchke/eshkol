;; Debug argument tracing for map operations


(display "=== ARGUMENT TRACE DEBUG ===")
(newline)

;; Test the problematic case step by step
(define list1 (list 1 2 3))
(define list2 (list 4 5 6))

;; First, test individual list element access
(display "List1 elements: ")
(display (car list1)) (display " ")
(display (cadr list1)) (display " ") 
(display (caddr list1))
(newline)

(display "List2 elements: ")
(display (car list2)) (display " ")
(display (cadr list2)) (display " ")
(display (caddr list2))
(newline)

;; Test individual + operations manually
(display "Manual arithmetic tests:")
(newline)
(display "1 + 4 = ") (display (+ 1 4))
(newline)
(display "2 + 5 = ") (display (+ 2 5))
(newline)
(display "3 + 6 = ") (display (+ 3 6))
(newline)

;; Test the problematic map call
(display "Now testing map + list1 list2:")
(newline)
(define corrupted-result (map + list1 list2))
(display "Result: ")
(display (car corrupted-result)) (display " ")
(display (cadr corrupted-result)) (display " ")
(display (caddr corrupted-result))
(newline)

(display "=== END ARGUMENT TRACE ===")
