;;
;; Phase 2A Simple Debug - Testing basic multi-list map step by step
;;

(require stdlib)

(display "=== PHASE 2A DEBUG TEST ===")
(newline)

;; Test 1: Single list map first (should work)
(display "1. Testing single-list map:")
(newline)
(define test-list (list 1 2))
(display "   List: 1 2")
(newline)

;; Test with simple built-in addition
(define single-result (map + test-list))
(display "   single map + result: ")
(if (null? single-result)
    (display "EMPTY")
    (begin
        (display (car single-result)) (display " ")
        (display (cadr single-result))
    ))
(newline)

(display "=== PHASE 2A DEBUG SUCCESS! ===")
(newline)
