(display "=== PHASE 1C REMOVE TEST ===")
(newline)

;; Test 1: Simple remove operation
(display "1. Testing remove function:")
(newline)
(define test-list (list 1 2 3))
(display "   Original list: 1 2 3")
(newline)

;; Remove 2
(define removed (remove 2 test-list))
(display "   remove 2: ")
(if (null? removed)
    (display "EMPTY")
    (begin
        (display (car removed)) (display " ")
        (display (cadr removed))
    ))
(newline)

(display "=== PHASE 1C REMOVE SUCCESS! ===")
(newline)
