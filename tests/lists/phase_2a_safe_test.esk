;;
;; Phase 2A Safe Test - Testing multi-list with safe access
;;


(display "=== PHASE 2A SAFE TEST ===")
(newline)

;; Test 1: Two-list map with 3 elements each
(display "1. Testing two 3-element lists:")
(newline)
(define list1 (list 1 2 3))
(define list2 (list 4 5 6))
(display "   List 1: 1 2 3")
(newline)
(display "   List 2: 4 5 6")
(newline)

;; Test multi-list map
(define result (map + list1 list2))
(display "   map + result: ")
(if (null? result)
    (display "EMPTY")
    (begin
        (display (car result))
        (if (null? (cdr result))
            (display "")
            (begin
                (display " ")
                (display (cadr result))
                (if (null? (cddr result))
                    (display "")
                    (begin
                        (display " ")
                        (display (car (cddr result)))
                    ))
            ))
    ))
(newline)

(display "=== PHASE 2A SAFE TEST COMPLETE ===")
(newline)
