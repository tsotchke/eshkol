;; Test let expressions step by step

;; Test 1: Simple let expression
(display "Test 1: ")
(let ((x 5))
  (display x))
(newline)

;; Test 2: Let with multiple bindings
(display "Test 2: ")
(let ((x 1) (y 2))
  (display (+ x y)))
(newline)

;; Test 3: Let with vector
(display "Test 3: ")
(let ((v (vector 1.0 2.0)))
  (display "vector in let"))
(newline)

;; Test 4: Lambda
(display "Test 4: ")
(define test-func (lambda (v) 0.0))
(display "lambda defined")
(newline)

;; Test 5: Call lambda
(display "Test 5: ")
(display (test-func 5.0))
(newline)

;; Test 6: Gradient
(display "Test 6: ")
(display (gradient test-func (vector 1.0 2.0)))
(newline)
