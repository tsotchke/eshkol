;;
;; Production Quality Test - No simplified implementations
;;

;; Test proper list creation
(define my-list (list 1 2 3 4 5))
(display "List first: ")
(display (car my-list))
(newline)
(display "List second: ")
(display (car (cdr my-list)))
(newline)
(display "List third: ")
(display (car (cdr (cdr my-list))))
(newline)

;; Test proper pair? predicate
(display "Is my-list a pair? ")
(display (pair? my-list))
(newline)
(display "Is 42 a pair? ")
(display (pair? 42))
(newline)
(display "Is 0 a pair? ")
(display (pair? 0))
(newline)

;; Test null? predicate
(display "Is empty list null? ")
(display (null? 0))
(newline)
(display "Is my-list null? ")
(display (null? my-list))
(newline)

(display "Production test completed!")
(newline)
