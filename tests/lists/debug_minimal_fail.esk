;;
;; Minimal failing case - gradually build towards exact failure
;;

(require stdlib)

(display "=== DEBUG MINIMAL FAIL TEST ===")
(newline)

;; Step 1: Just the map operation (no display of results)
(define list1 (list 1 2 3))
(define list2 (list 4 5 6))
(define result1 (map + list1 list2))

(display "Map operation completed")
(newline)

;; Step 2: Access first element only
(display "First element: ")
(display (car result1))
(newline)

;; Step 3: Access second element
(display "Second element: ")
(display (cadr result1))
(newline)

;; Step 4: This is where the crash might happen - third element access
(display "About to access third element...")
(newline)
(display "Third element: ")
(display (caddr result1))
(newline)

(display "=== MINIMAL FAIL TEST COMPLETE ===")
