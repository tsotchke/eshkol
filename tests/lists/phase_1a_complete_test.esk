;;
;; Phase 1A Complete Implementation Test
;; Testing all newly implemented functions: for-each, assoc/assq/assv, list*, acons
;;

(display "=== PHASE 1A COMPLETE TEST ===")
(newline)

;; Test 1: For-each function
(display "1. Testing for-each:")
(newline)
(define numbers (list 5 10 15))
(display "   Numbers: ") (display (car numbers)) (display " ") (display (cadr numbers)) (display " ") (display (caddr numbers))
(newline)
(display "   for-each display: ")
(for-each display numbers)
(newline)

;; Test 2: Association list functions
(display "2. Testing association lists:")
(newline)
(define alist (list (cons 100 1000) (cons 200 2000) (cons 300 3000)))
(display "   Created alist with pairs: (100,1000) (200,2000) (300,3000)")
(newline)

(define found-200 (assoc 200 alist))
(display "   assoc 200: ")
(if (null? found-200) (display "NOT FOUND") (display "FOUND"))
(newline)

(define not-found (assoc 999 alist))
(display "   assoc 999: ")
(if (null? not-found) (display "NOT FOUND") (display "FOUND"))
(newline)

;; Test 3: Acons function
(display "3. Testing acons:")
(newline)
(define extended-alist (acons 400 4000 alist))
(display "   acons 400 4000: Added new pair to alist")
(newline)
(define found-400 (assoc 400 extended-alist))
(display "   assoc 400 in extended: ")
(if (null? found-400) (display "NOT FOUND") (display "FOUND"))
(newline)

;; Test 4: List* improper lists
(display "4. Testing list* improper lists:")
(newline)
(define improper (list* 7 8 9 999))
(display "   list* 7 8 9 999 created")
(newline)
(display "   Elements: ") (display (car improper)) (display " ") (display (cadr improper)) (display " ") (display (caddr improper))
(newline)
(display "   Terminal: ") (display (cdr (cdr (cdr improper))))
(newline)

;; Test 5: Mixed functionality test
(display "5. Testing combined functionality:")
(newline)
(define test-list (list 1 2 3 4 5))
(define doubled-list (map + test-list))
(display "   Original: ") (display (car test-list)) (display " ") (display (cadr test-list)) (display " ") (display (caddr test-list))
(newline)
(display "   Doubled:  ") (display (car doubled-list)) (display " ") (display (cadr doubled-list)) (display " ") (display (caddr doubled-list))
(newline)

(display "=== PHASE 1A COMPLETE SUCCESS! ===")
(newline)
