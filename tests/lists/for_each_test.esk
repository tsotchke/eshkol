;;
;; For-Each Function Test
;;

(require stdlib)

(display "Testing for-each function:")
(newline)

;; Create a simple list
(define numbers (list 1 2 3 4))
(display "Original list: ")
(display (car numbers)) (display " ")
(display (cadr numbers)) (display " ")
(display (caddr numbers)) (display " ")
(display (car (cdr (cdr (cdr numbers)))))
(newline)

;; Test for-each with print (side effects)
(display "for-each with print: ")
(for-each print numbers)
(newline)

(display "for-each test completed!")
(newline)
