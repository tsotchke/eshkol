;;; Test compose only

(define (compose f g)
  (lambda (x) (f (g x))))

(define add1 (lambda (x) (+ x 1)))
(define double (lambda (x) (* x 2)))

(display "Testing compose:")
(newline)

(define add1-then-double (compose double add1))

(display "add1-then-double 5 = ")
(display (add1-then-double 5))  ; (5+1)*2 = 12
(newline)
