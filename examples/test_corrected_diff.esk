;; Test the corrected differentiation implementation

(define (main)
    (printf "=== Corrected Differentiation Test ===\n")
    
    ;; Test derivative of constant (should be 0)
    (printf "d/dx(42) = %ld (should be 0)\n" (diff 42 x))
    
    ;; Test derivative of variable (should be 1)
    (printf "d/dx(x) = %ld (should be 1)\n" (diff x x))
    
    ;; Test derivative of different variable (should be 0)
    (printf "d/dx(y) = %ld (should be 0)\n" (diff y x))
    
    (printf "=== Test Complete ===\n")
    0)