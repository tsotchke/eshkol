;; Simple test of tensor-apply functionality

(define (main)
  ;; Create vector with first element 5
  (define v1 (vector 5 10 15))
  
  ;; Apply square function (5*5 = 25)
  (define v_squared (tensor-apply v1 square))
  
  ;; Test tensor-reduce with addition (0 + 5 = 5)
  (define result (tensor-reduce v1 + 0))
  
  ;; Return the result as exit code (should be 5)
  result)