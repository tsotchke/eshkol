;; Test differentiation with actual values

(define (derivative_test x)
    ;; d/dx(x * x) should return 2x
    (diff (* x x) x))

(define (main)
    (printf "Testing d/dx(x * x) with actual values:\n")
    (printf "At x=3: d/dx(x^2) = %ld (should be 2*3 = 6)\n" (derivative_test 3))
    (printf "At x=5: d/dx(x^2) = %ld (should be 2*5 = 10)\n" (derivative_test 5))
    0)