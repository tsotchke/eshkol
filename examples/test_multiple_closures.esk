(define (make-adder n)
  (lambda (x) (+ x n)))

(define (make-multiplier n)
  (lambda (x) (* x n)))

(define (main)
  (define add3 (make-adder 3))
  (define add7 (make-adder 7))
  (define mult2 (make-multiplier 2))
  
  (printf "add3(10): %d\n" (add3 10))
  (printf "add7(5): %d\n" (add7 5))
  (printf "mult2(6): %d\n" (mult2 6))
  
  0)