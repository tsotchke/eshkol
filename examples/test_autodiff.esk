;; Test file for symbolic automatic differentiation

(define (main)
    (printf "=== Symbolic Automatic Differentiation Test ===\n")
    
    ;; Test basic differentiation
    (printf "Testing diff function...\n")
    
    ;; Test: d/dx(x) = 1
    (printf "d/dx(x) = %ld\n" (diff x x))
    
    ;; Test: d/dx(5) = 0 
    (printf "d/dx(5) = %ld\n" (diff 5 x))
    
    ;; Test: d/dx(y) = 0 (where y is not x)
    (printf "d/dx(y) = %ld\n" (diff y x))
    
    (printf "=== Basic Differentiation Test Complete ===\n")
    0)